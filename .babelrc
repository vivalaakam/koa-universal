{
  "presets": [
    "es2015",
    "stage-0",
    "stage-2",
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-async-to-generator",
    "transform-decorators-legacy",
    "transform-runtime"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
